<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <VFEPirates.WarcasketDef Name="VFEP_WarcasketPartBase" Abstract="True">
    <tradeability>None</tradeability>
	<thingClass>VFEPirates.Apparel_Warcasket</thingClass>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <category>Item</category>
    <drawerType>MapMeshOnly</drawerType>
    <selectable>True</selectable>
    <pathCost>14</pathCost>
    <useHitPoints>False</useHitPoints>
    <techLevel>Industrial</techLevel>
    <drawGUIOverlay>true</drawGUIOverlay>
	<destroyOnDrop>true</destroyOnDrop>
    <statBases>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>0</DeteriorationRate>
      <Beauty>-3</Beauty>
	  <WorkToMake>1000</WorkToMake>
    </statBases>
    <apparel>
      <useDeflectMetalEffect>true</useDeflectMetalEffect>
    </apparel>
	<generateCommonality>0</generateCommonality>
    <altitudeLayer>Item</altitudeLayer>
    <alwaysHaulable>True</alwaysHaulable>
    <tickerType>Never</tickerType>
    <burnableByRecipe>false</burnableByRecipe>
    <smeltable>false</smeltable>
    <thingSetMakerTags>
      <li>Warcasket</li> <!-- Won't be actually used anywhere -->
    </thingSetMakerTags>
    <tradeTags>
      <li>Warcasket</li> <!-- Won't be actually used anywhere -->
    </tradeTags>
    <comps>
      <li Class="CompProperties_Forbiddable"/>
      <li>
        <compClass>CompColorable</compClass>
      </li>
    </comps>
  </VFEPirates.WarcasketDef>
  
  <VFEPirates.WarcasketDef ParentName="VFEP_WarcasketPartBase" Name="VFEP_WarcasketArmorBase" Abstract="True">
	<thingClass>VFEPirates.Apparel_WarcasketArmor</thingClass>
	<isArmor>true</isArmor>
	<shortDescription>short desc</shortDescription>
	<modExtensions>
		<li Class="VFECore.ApparelDrawPosExtension">
			<shellPosDrawSettings>
				<drawPosNorthOffset>(0, 0.1, 0)</drawPosNorthOffset>
				<drawPosSouthOffset>(0, 0, 0)</drawPosSouthOffset>
				<drawPosWestOffset>(-0.03, 0, 0)</drawPosWestOffset>
				<drawPosEastOffset>(0.03, 0, 0)</drawPosEastOffset>
				<!--
				<drawNorthSize>(2, 2)</drawNorthSize>
				<drawSouthSize>(2, 2)</drawSouthSize>
				<drawWestSize>(2, 2)</drawWestSize>
				<drawEastSize>(2, 2)</drawEastSize>-->
			</shellPosDrawSettings>
			<isUnifiedApparel>true</isUnifiedApparel>
			<showBodyInBedAlways>true</showBodyInBedAlways>
		</li>
	</modExtensions>
  </VFEPirates.WarcasketDef>
  
